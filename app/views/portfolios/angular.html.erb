<h1>Angular Portfolio</h1>

<% @angular_items.each do |portfolio| %>
  <h3><%= link_to portfolio.title, portfolio_show_path(portfolio) %></h3>
  <h5><%= portfolio.sub_title%></h5>
  <p><%= portfolio.body %></p>
  <p><%=  image_tag  portfolio.thumb_image unless portfolio.thumb_image.nil?  %></p>
  <%= link_to "Edit", edit_portfolio_path(portfolio) %>
  <%= link_to "Delete Portfolio", portfolio_path(portfolio), method: :delete,data: {confirm:  "Are you sure? " }%>
<% end %>